{
    "id": "todoapp",
    "name": "TodoApp",
    "iosPath": "App/App.xcodeproj",
    "builds": {
        "ios": {
            "appId": "io.ionic.todoapp",
            "buildFlag": [
                "-sdk",
                "iphonesimulator"
            ],
            "automaticProvisioning": true,
            "developmentTeam": "auto",
            "env": {
                "SIMULATOR_BUILD": "true"
            },
            "options": {
                "buildType": "simulator",
                "emulator": true
            }
        }
    },
    "options": {
        "simulator": true,
        "includeSourcemaps": true
    },
    "hooks": {
        "prebuild": [],
        "postbuild": []
    }
}